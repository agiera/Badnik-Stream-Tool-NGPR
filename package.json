{
  "name": "badnik-st-gui",
  "version": "1.0.0",
  "description": "An electron interface for Project+ and Melee tournament streams.",
  "main": "index.js",
  "repository": "https://github.com/Motobug/Badnik-Stream-Tool",
  "author": "Motobug, Readek, & ilikepizza107",
  "license": "MIT",
  "devDependencies": {
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12"
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "badnik.st.gui",
    "linux": {
      "target": "AppImage"
    },
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "signAndEditExecutable": false
    },
    "mac": {
      "target": "dmg"
    }
  },
  "dependencies": {
    "ws": "^8.18.3"
  }
}
